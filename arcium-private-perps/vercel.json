{
  "version": 2,
  "buildCommand": "echo 'No build needed - static site'",
  "outputDirectory": ".",
  "installCommand": "echo 'No install needed'",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/html"
        }
      ]
    }
  ]
}
